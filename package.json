{
  "name": "qraftbox",
  "version": "0.0.4",
  "description": "QraftBox - AI-powered local git diff workspace",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "qraftbox": "./bin/qraftbox.js"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/main.ts",
    "build": "bun build src/main.ts --outdir dist --target bun",
    "start": "bun run src/main.ts",
    "test": "vitest run",
    "test:e2e": "playwright test -c playwright.config.cjs",
    "test:all": "bun run test && bun run test:e2e",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "bun run typecheck",
    "format": "prettier --write \"src/**/*.ts\" \"client/**/*.svelte\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"client/**/*.svelte\"",
    "clean": "rm -rf dist"
  },
  "homepage": "https://github.com/tacogips/QraftBox",
  "repository": {
    "type": "git",
    "url": "https://github.com/tacogips/QraftBox.git"
  },
  "license": "MIT",
  "trustedDependencies": [
    "codex-agent"
  ],
  "devDependencies": {
    "@playwright/test": "1.58.1",
    "@types/bun": "1.1.14",
    "@types/mustache": "4.2.6",
    "@vitest/ui": "2.1.8",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.4.1",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@octokit/rest": "22.0.1",
    "claude-code-agent": "github:tacogips/claude-code-agent#4f9d8f0debc8b195124de020f76e0cbbfccb3684",
    "codex-agent": "github:tacogips/codex-agent#ea567f282d3ecd5aa06abf68e408f7403d69ebc9",
    "commander": "14.0.3",
    "hono": "4.11.7"
  }
}
