{
  "name": "qraftbox",
  "version": "0.0.1",
  "description": "All You Need Is Diff",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "bin": {
    "qraftbox": "./bin/qraftbox.js"
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/main.ts",
    "build": "bun build src/main.ts --outdir dist --target bun",
    "start": "bun run src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "bun run typecheck",
    "format": "prettier --write \"src/**/*.ts\" \"client/**/*.svelte\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"client/**/*.svelte\"",
    "clean": "rm -rf dist"
  },
  "homepage": "https://github.com/user/repo",
  "repository": {
    "type": "git",
    "url": "https://github.com/user/repo"
  },
  "license": "MIT",
  "trustedDependencies": [],
  "devDependencies": {
    "@playwright/test": "1.58.1",
    "@types/bun": "1.1.14",
    "@types/mustache": "4.2.6",
    "@vitest/ui": "2.1.8",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.4.1",
    "typescript": "5.7.2",
    "vitest": "2.1.9"
  },
  "dependencies": {
    "@octokit/rest": "22.0.1",
    "claude-code-agent": "github:tacogips/claude-code-agent",
    "commander": "14.0.3",
    "hono": "4.11.7"
  }
}
